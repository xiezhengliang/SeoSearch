<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nisure.mapper.userAgent">
	<select id="getUserAgent" parameterType="String" resultType="com.nisure.model.UserAgentEntity">
		select * from user_agent order by rand() limit 1
	</select>
	
	
</mapper>